<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="property_tag_actions_id" model="ir.actions.act_window">
        <field name="name">Property Tags</field>
        <field name="res_model">estate.property.tag</field>
    </record>

    <record id="property_tag_form_view_id" model="ir.ui.view">
        <field name="name">property_tag_form_view_id</field>
        <field name="model">estate.property.tag</field>
        <field name="arch" type="xml">
            <form string="Property Tag">
                <sheet>
                    <group>
                        <field name="name" />                            
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
